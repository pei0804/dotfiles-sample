! switch Zenkaku_Hankaku to Escape keys
! keycode 9 = Zenkaku_Hankaku
! keycode 49 = Escape

! change Muhenkan to Return
! keycode 102 = Return

! change Henkan to Zenkaku_Hankaku
! keycode 100 = Zenkaku_Hankaku

!switch Control_L to Caps_Lock
! keycode 66 =Control_L
! keycode 37 = Caps_Lock

! for ubuntu
remove Control = Control_L
add Lock = Caps_Lock
add Control = Control_L
keycode 100 = Return
keycode 102 = Zenkaku_Hankaku
